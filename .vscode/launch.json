{
  "version": "0.2.0",
  "configurations": [
    {
      "type": "lldb",
      "request": "launch",
      "name": "Debug",
      "cargo": {
        "args": ["build", "--bin=memochan", "--package=memochan"],
        "filter": {
          "name": "memochan",
          "kind": "bin"
        }
      },
      "args": [],
      "cwd": "${workspaceFolder}",
      "preLaunchTask": "build"
    },
    {
      "type": "lldb",
      "request": "launch",
      "name": "Debug (Release)",
      "cargo": {
        "args": ["build", "--release", "--bin=memochan", "--package=memochan"],
        "filter": {
          "name": "memochan",
          "kind": "bin"
        }
      },
      "args": [],
      "cwd": "${workspaceFolder}",
      "preLaunchTask": "build release"
    }
  ]
}
